# NDK_TOOLCHAIN_VERSION := clang3.6

APP_CPPFLAGS += -Wno-deprecated 
APP_CPPFLAGS += -std=c++11
APP_CPPFLAGS += -frtti
APP_CPPFLAGS += -fexceptions
APP_CPPFLAGS += -DHAVE_PTHREAD=1
APP_CPPFLAGS += -DUSE_EIGEN_GEMM
APP_CFLAGS += -D__USE_SDL_GLES__
APP_CFLAGS += -D_REENTRANT
APP_CFLAGS += -D_THREAD_SAFE
APP_CFLAGS += -DEIGEN_DONT_VECTORIZE
APP_CFLAGS += -DLASZIPDLL_EXPORTS
APP_CFLAGS += -DUNORDERED
APP_STL := gnustl_shared
