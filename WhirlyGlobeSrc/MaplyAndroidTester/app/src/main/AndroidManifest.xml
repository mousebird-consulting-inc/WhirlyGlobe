<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mousebirdconsulting.maplyandroidtester" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MapActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:enabled="true" android:name="com.mousebirdconsulting.maplyandroidtester.GdbServerService"
            android:label="@string/app_name" android:icon="@mipmap/ic_launcher">
            <intent-filter >
                <action android:name="com.mousebirdconsulting.maplyandroidtester.GdbServerService" />
            </intent-filter>
        </service>

    </application>
</manifest>
